<% if facet.values.any? %>
  <% facet.values.each do |year| %>
    <% if facet.value_selected?(year) %>
      <%= render  'faceted_search/facets/facet-selected',
                  title: facet.title,
                  value: year,
                  path: facet.facets.path_for(facet, year) %>
    <% end %>
  <% end %>
<% end %>
